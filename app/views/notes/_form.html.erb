<%= form_with(model: @note, local: true) do |form| %>
  <div class="note_title">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div class="note_field">
    <%= form.label :image %>
    <%= form.file_field :image %>
  </div>
  <div class="note_content">
    <%= form.label :content %>
    <%= form.text_field :content %>
  </div>
  <%= form.submit %>
<% end %>
